{
    "name": "alexregenbogen/laravel-comments-ratings-reviews",
    "description": "Add comments/ratings & reviews to your Laravel application",
    "keywords": [
        "comments",
        "ratings",
        "reviews",
        "laravel-comments-ratings-reviews"
    ],
    "homepage": "https://github.com/AlexRegenbogen/laravel-comments-ratings-reviews/",
    "license": "MIT",
    "authors": [
        {
            "name": "Alex Regenbogen",
            "email": "github@relexed.com",
            "homepage": "https://relexed.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "~5.6.0|~5.7.0|~5.8.0|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.6|^5.0|^6.0|^7.0|^8.0",
        "phpunit/phpunit": "^7.0|^8.0|9.2.*|^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "AlexRegenbogen\\CommentsRatingsReviews\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AlexRegenbogen\\CommentsRatingsReviews\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "AlexRegenbogen\\CommentsRatingsReviews\\CommentsRatingsReviewsServiceProvider"
            ]
        }
    }
}
